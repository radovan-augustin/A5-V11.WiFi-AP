config wifi-device 'wdev'
        option type 'mac80211'
        option channel '11'
        option hwmode '11g'
        option path 'platform/101c0000.ehci/usb1/1-1/1-1:1.0'
        option htmode 'HT20'
        option disabled '0'

config wifi-iface 'wintf'
        option device 'wdev'
        option network 'lan'
        option mode 'ap'
        option ssid 'LEDE'
        option encryption 'psk2+ccmp'
        option key 'password'
